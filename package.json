{
	"name": "maxstack",
	"version": "0.2.0",
	"description": "The AI-First Full-Stack Framework - Build production-ready React applications with AI-powered development, built-in templates, and modern tooling",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sys13/maxstack.git"
	},
	"license": "MIT",
	"author": {
		"name": "Daniel Arrizza",
		"email": "daniel.arrizza@gmail.com"
	},
	"type": "module",
	"bin": {
		"maxstack": "bin/index.js"
	},
	"files": [
		"LICENSE.md",
		"README.md",
		"bin/",
		"lib/",
		"package.json",
		"template/"
	],
	"scripts": {
		"build": "tsdown",
		"format": "prettier . --write",
		"lint": "eslint --fix . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"pretest": "pnpm run build",
		"test": "vitest",
		"tsc": "tsc",
		"typecheck": "tsc --noEmit",
		"validate": "pnpm run lint && pnpm run lint:knip && pnpm run lint:md && pnpm run lint:packages && pnpm run lint:spelling && pnpm run typecheck && pnpm run test run"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@inquirer/prompts": "^7.8.0",
		"commander": "^14.0.0",
		"es-toolkit": "^1.39.8",
		"esbuild": "^0.25.8",
		"ts-morph": "^26.0.0",
		"zod": "^4.0.15"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
		"@eslint/js": "9.32.0",
		"@paralleldrive/cuid2": "^2.2.2",
		"@release-it/conventional-changelog": "10.0.1",
		"@types/eslint-plugin-markdown": "2.0.2",
		"@types/node": "24.2.0",
		"@vitest/coverage-v8": "3.2.4",
		"@vitest/eslint-plugin": "1.3.4",
		"console-fail-test": "0.5.0",
		"cspell": "9.2.0",
		"drizzle-orm": "1.0.0-beta.1-c0277c0",
		"eslint": "9.32.0",
		"eslint-plugin-jsdoc": "52.0.4",
		"eslint-plugin-jsonc": "2.20.1",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.21.3",
		"eslint-plugin-package-json": "0.51.0",
		"eslint-plugin-regexp": "2.9.1",
		"eslint-plugin-yml": "1.18.0",
		"execa": "^9.6.0",
		"husky": "9.1.7",
		"knip": "5.62.0",
		"lint-staged": "16.1.4",
		"markdownlint": "0.38.0",
		"markdownlint-cli": "0.45.0",
		"prettier": "3.6.2",
		"prettier-plugin-curly": "0.3.2",
		"prettier-plugin-packagejson": "2.5.19",
		"prettier-plugin-sh": "0.18.0",
		"release-it": "19.0.4",
		"sentences-per-line": "0.3.0",
		"tsdown": "0.13.3",
		"typescript": "5.9.2",
		"typescript-eslint": "8.39.0",
		"vitest": "3.2.4"
	},
	"packageManager": "pnpm@10.14.0",
	"engines": {
		"node": ">=20.19.0"
	},
	"publishConfig": {
		"provenance": true
	}
}
